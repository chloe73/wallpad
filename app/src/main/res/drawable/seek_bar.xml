<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <!--    seek bar 배경-->
    <item android:id="@+id/background">
        <shape android:shape="rectangle">
            <corners
                android:radius="3.5dp"/>
            <solid
                android:color="#464646" />
            <size
                android:height="7px"/>
        </shape>
    </item>
    <!--시크바 프로그래스-->
    <item android:id="@android:id/progress">
        <clip>
            <shape android:shape="rectangle">
                <corners android:radius="3.5dp" />
<!--                <stroke-->
<!--                    android:width="7px"-->
<!--                    android:color="#7429e8"/>-->
                <gradient
                    android:angle="0"
                    android:startColor="#7429e8"
                    android:centerColor="#725bea"
                    android:endColor="#c46cf4"
                    android:type="linear"/>
                <size
                    android:height="7px"/>
            </shape>
        </clip>
    </item>

</layer-list>