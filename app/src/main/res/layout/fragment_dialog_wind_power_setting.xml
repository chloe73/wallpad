<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".fragment.ventilation.DialogWindPowerSettingFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/black"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent">
        <ImageView
            android:id="@+id/fragment_wind_power_setting_box"
            android:layout_width="881px"
            android:layout_height="794px"
            android:background="@drawable/dialog_background"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/btn_close_in_wind_power_setting_dialog"
            android:layout_width="24px"
            android:layout_height="24px"
            android:layout_margin="42px"
            android:layout_marginEnd="42px"
            android:layout_marginTop="42px"
            android:background="@drawable/btn_close"
            app:layout_constraintEnd_toEndOf="@id/fragment_wind_power_setting_box"
            app:layout_constraintTop_toTopOf="@id/fragment_wind_power_setting_box" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="278px"
            android:layout_height="93px"
            android:layout_margin="82px"
            android:background="@drawable/dialog_title_background"
            app:layout_constraintEnd_toEndOf="@id/fragment_wind_power_setting_box"
            app:layout_constraintStart_toStartOf="@id/fragment_wind_power_setting_box"
            app:layout_constraintTop_toTopOf="@id/fragment_wind_power_setting_box" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/apple_sd_gothic_neo_sb"
            android:text="풍량"
            android:textColor="@color/white"
            android:textSize="50px"
            android:textStyle="normal"
            app:layout_constraintTop_toTopOf="@+id/imageView2"
            app:layout_constraintStart_toStartOf="@+id/imageView2"
            app:layout_constraintEnd_toEndOf="@+id/imageView2"
            app:layout_constraintBottom_toBottomOf="@+id/imageView2"/>

        <ImageView
            android:id="@+id/img_dialog_contents"
            android:layout_width="770px"
            android:layout_height="504px"
            android:layout_marginTop="61px"
            android:background="@drawable/pop_up_inner_box"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView2" />

        <TextView
            android:id="@+id/tv_normal_ventilation"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="69px"
            android:layout_marginStart="49px"
            android:fontFamily="@font/apple_sd_gothic_neo_m"
            android:text="미풍"
            android:textColor="@color/white"
            android:textSize="37px"
            android:textStyle="normal"
            app:layout_constraintStart_toStartOf="@+id/img_dialog_contents"
            app:layout_constraintTop_toTopOf="@+id/img_dialog_contents" />

        <Switch
            android:layout_width="132px"
            android:layout_height="61px"
            android:layout_marginTop="60.5px"
            android:layout_marginEnd="51px"
            android:layout_marginBottom="4dp"
            android:thumb="@drawable/switch_thumb"
            android:track="@drawable/switch_track"
            app:layout_constraintEnd_toEndOf="@id/img_dialog_contents"
            app:layout_constraintTop_toTopOf="@+id/img_dialog_contents" />

        <ImageView
            android:id="@+id/img_line1"
            android:layout_width="688.9px"
            android:layout_height="3px"
            android:layout_marginTop="55.5px"
            android:background="@drawable/img_line"
            app:layout_constraintEnd_toEndOf="@+id/img_dialog_contents"
            app:layout_constraintStart_toStartOf="@+id/img_dialog_contents"
            app:layout_constraintTop_toBottomOf="@+id/tv_normal_ventilation" />

        <TextView
            android:id="@+id/tv_natural_ventilation"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="49px"
            android:layout_marginTop="59.5px"
            android:fontFamily="@font/apple_sd_gothic_neo_m"
            android:text="약풍"
            android:textColor="@color/white"
            android:textSize="37px"
            android:textStyle="normal"
            app:layout_constraintStart_toStartOf="@+id/img_dialog_contents"
            app:layout_constraintTop_toBottomOf="@+id/img_line1" />

        <Switch
            android:layout_width="132px"
            android:layout_height="61px"
            android:layout_marginTop="60.5px"
            android:layout_marginEnd="51px"
            android:layout_marginBottom="4dp"
            android:thumb="@drawable/switch_thumb"
            android:track="@drawable/switch_track"
            app:layout_constraintEnd_toEndOf="@id/img_dialog_contents"
            app:layout_constraintTop_toBottomOf="@+id/img_line1" />

        <ImageView
            android:id="@+id/img_line2"
            android:layout_width="688.9px"
            android:layout_height="3px"
            android:layout_marginTop="53.7px"
            android:background="@drawable/img_line"
            app:layout_constraintEnd_toEndOf="@+id/img_dialog_contents"
            app:layout_constraintStart_toStartOf="@+id/img_dialog_contents"
            app:layout_constraintTop_toBottomOf="@+id/tv_natural_ventilation" />

        <TextView
            android:id="@+id/tv_inner_ventilation"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="강풍"
            android:layout_marginStart="49px"
            android:layout_marginTop="66.3px"
            android:textStyle="normal"
            android:textSize="37px"
            android:textColor="@color/white"
            app:layout_constraintTop_toBottomOf="@+id/img_line2"
            app:layout_constraintStart_toStartOf="@id/img_dialog_contents"
            />

        <Switch
            android:layout_width="132px"
            android:layout_height="61px"
            android:layout_marginTop="60.5px"
            android:layout_marginEnd="51px"
            android:layout_marginBottom="4dp"
            android:thumb="@drawable/switch_thumb"
            android:track="@drawable/switch_track"
            app:layout_constraintEnd_toEndOf="@id/img_dialog_contents"
            app:layout_constraintTop_toBottomOf="@+id/img_line2" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>